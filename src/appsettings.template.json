{
  "CachePolicy": {
    "PolicyName": "UrlMapCache",
    "TagName": "UrlMapTag",
    "Expiration": 1,
    "QueryKeys": ["ShortName"]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "x-api-key": "my-x-api-key",
  "Database": {
    "ConnectionString": "DataSource=./Database/app.db"
  }
}
